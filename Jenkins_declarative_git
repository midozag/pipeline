pipeline{
    agent any
    environment{
      PATH = "/opt/apache-maven-3.8.1/bin:$PATH"

    }
    stages{
      stage("Git checkout"){
          steps{
            git credentialsId: 'github', url: 'https://github.com/midozag/pipeline'  
          }
      }  
      stage("Maven Build"){
        steps{
           sh "mvn clean package"
        }
      
      }
        
        
    }
    
    
    
}
